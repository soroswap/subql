    "dist",
    "schema.graphql",
    "project.yaml"
  ],
  "author": "PrincesoDan",
  "license": "MIT",
  "dependencies": {
    "@subql/common": "latest",
    "@subql/types-stellar": "latest"
  },
  "devDependencies": {
    "@subql/cli": "latest",
    "@subql/testing": "latest",
    "@subql/types": "latest",
    "@types/node": "^22.13.1",
    "dotenv": "latest",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
